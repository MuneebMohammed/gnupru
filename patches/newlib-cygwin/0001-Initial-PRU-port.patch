+ * Modifications to this software may be copyrighted by their authors
+ * and need not follow the licensing terms described here, provided that
+ * the new terms are clearly indicated on the first page of each file where
+ * they apply.
+ */
+#include "newlib.h"
+
+	.extern	_main
+	.extern	exit
+
+	.text
+	.section .init0, "x"
+	.global	__start
+__start:
+	/* Initialize stack pointer. */
+	ldi	sp, %lo(__stack_top)
+	ldi	sp.w2, %hi_rlz(__stack_top)
+
+	/* DATA and BSS are handled by the loader, so nothing to do here. */
+
+#if !defined(MINRT)
+	.extern __do_global_dtors
+	/* Ensure destructors get called. */
+	ldi	r14, %pmem(__do_global_dtors)
+	call	_atexit
+
+	/* Call constructors. Use non-call-clobbered registers. */
+	ldi	r5, %lo(__ctors_start)
+	ldi	r5.w2, %hi_rlz(__ctors_start)
+	ldi	r6, %lo(__ctors_end)
+	ldi	r6.w2, %hi_rlz(__ctors_end)
+ctors_loop:
+	qbeq	ctors_done, r5, r6
+	lbbo	r7, r5, 0, 4		/* IMEM pointers are 32bits for now */
+	call	r7
+	add	r5, r5, 4
+	jmp	ctors_loop
+ctors_done:
+#endif
+	
+	/* Just in case main() tries to access argc, argv[] and envp. */
+	zero	r14, 3 * 4
+
+	/* Call main */
+	call	_main
+
+#if !defined(MINRT)
+	/* Call exit */
+	call	_exit
+#endif
+
+	/* We should never reach here. */
+__crt_exit:
+	halt
+	jmp	__crt_exit
