+#include "newlib.h"
+
+#define SYSCALL_VALUE	35  /* (1 << 5) | (19 - 16) = 35 */
+
+
+	/* Due to ABI, all syscalls with up tp 15 arguments contain
+	   the same insturction sequences. So we squash them to save space. */
+	.text
